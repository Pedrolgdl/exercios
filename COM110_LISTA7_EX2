
#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
    

int main(void) {
  int mat[8][8], linhas, colunas;
  bool test = false;

printf("\n\n  Digite quantas linhas a matriz terá: ");
scanf("%d", &linhas);
printf("\n\n  Digite quantas colunas a matriz terá: ");
scanf("%d", &colunas);
printf("\n\n");
    for(int i = 1; i<=linhas;i++){
      for(int j=1; j<=colunas;j++){
       mat[i][j] = rand()%50;

       printf("%d  ", mat[i][j]) ;
      }
      printf("\n");
    }
    for(int i = 1; i<linhas ; i++){
      for(int j =1; i<colunas; i++){
       
        if( mat[i][j] != mat[j][i]){
         test = true;
         }
         else if(mat[i][j] == mat[j][i]){
           test = false;
         }
      }
  
    }

    if( test == true){
      printf("\n\n Essa matriz é simetrica. ");
    } 
    else if( test == false){
      printf("\n\n Essa matriz não é simetrica. ");
    }
  return 0;
}

     
  
    
      

  

